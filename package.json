{
  "name": "react-hanji-slider",
  "version": "0.0.1",
  "description": "A React Component for Interactive Comparisons",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c rollup.config.start.js",
    "start": "rollup -c rollup.config.start.js -w",
    "dev": "rollup -c rollup.config.dev.js -w",
    "lint": "eslint \"src/**/*.{js,tsx}\" --fix --quiet",
    "pretest": "npm run lint"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {}
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },

    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__tests/mockCss.js",
      "^(components/.+)$": "<rootDir>/src/$1/"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "automock": false,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gosu-Team/react-hanji-slider.git"
  },
  "keywords": [
    "react",
    "carousel",
    "react-component",
    "reactjs",
    "carousel",
    "slider",
    "ticker"
  ],
  "author": "Gosu.team <meta.ml@proton.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gosu-Team/react-hanji-slider/issues"
  },
  "homepage": "https://github.com/Gosu-Team/react-hanji-slider#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.23",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.31.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.19",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 || 18.0.0",
    "react-dom": ">= 16.8.0 || 18.0.0"
  },
  "files": [
    "dist"
  ]
}
